{
  "name": "refaq",
  "displayName": "refaq",
  "description": "",
  "version": "0.0.1",
  "publisher": "chmos",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:hmi.welcome",
    "onCommand:hmi.stub1",
    "onCommand:hmi.stub2",
    "onCommand:hmi.stub_comp"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hmi.welcome",
        "title": "hmi welcome"
      },
      {
        "command": "hmi.stub1",
        "title": "Copy to stub1"
      },
      {
        "command": "hmi.stub2",
        "title": "Copy to stub2"
      },
      {
        "command": "hmi.stub_comp",
        "title": "Compare stubs"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "hmi.stub1",
          "group": "hmi"
        },
        {
          "command": "hmi.stub2",
          "group": "hmi"
        },
        {
          "command": "hmi.stub_comp",
          "group": "hmi@2"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.51.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
